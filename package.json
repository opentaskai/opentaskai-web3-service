{
    "name": "opentaskai-web3-service",
    "version": "1.0.0",
    "description": "opentaskapi web3 service",
    "license": "MIT",
    "author": "opentaskapi team",
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "directories": {
        "doc": "docs",
        "lib": "lib"
    },
    "scripts": {
        "build": "npx tsc",
        "dev": "ts-node --files",
        "check-types:incremental": "npm run check-types --incremental",
        "devwww": "nodemon ./src/www.ts",
        "eslint:check": "eslint . --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
        "lint": "concurrently \"npm run check-types\" \"npm run eslint:check\" \"npm run prettier:check\" \"npm run prettier:check:package.json\"",
        "prepare": "husky install && npm run build",
        "prettier:check": "prettier --check .",
        "test": "mocha --timeout 1000000 --require ts-node/register",
        "testall": "mocha --timeout 100000 --require ts-node/register tests/**/*.test.ts",
        "prettier:check:package.json": "prettier-package-json --list-different",
        "prettier:fix": "prettier --write ."
    },
    "dependencies": {
        "axios": "^0.27.2",
        "bson": "^5.3.0",
        "cookie-parser": "^1.4.6",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.0.1",
        "ethers": "^5.7.1",
        "express": "^4.18.1",
        "express-jwt": "^8.4.1",
        "js-base64": "^3.7.2",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "minimist": "^1.2.6",
        "moment": "^2.29.4",
        "mongodb": "^4.8.1",
        "nodemon": "^2.0.19",
        "opentaskai-web3-jssdk": "^0.0.15",
        "request-ip": "^3.3.0",
        "sharp": "^0.32.6",
        "sleep-ts": "^0.1.1",
        "stream": "^0.0.2",
        "ts-custom-error": "^3.2.0",
        "tunnel": "^0.0.6",
        "url": "^0.11.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/chai-as-promised": "^7.1.5",
        "@types/cookie-parser": "^1.4.6",
        "@types/crypto-js": "^4.2.1",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/lodash": "^4.14.185",
        "@types/minimist": "^1.2.2",
        "@types/mocha": "^9.1.1",
        "@types/node": "^12.20.47",
        "@types/request-ip": "^0.0.41",
        "@types/tunnel": "^0.0.3",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "chai": "^4.3.6",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^7.1.0",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-n": "^15.2.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-promise": "^6.0.0",
        "husky": ">=6",
        "lint-staged": ">=10",
        "mocha": "^10.0.0",
        "prettier": "^2.5.1",
        "prettier-package-json": "^2.6.3",
        "prettier-plugin-solidity": "^1.0.0-beta.13",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}
